{
  "name": "vscode-react-component-generator",
  "displayName": "VSCode React Component Generator",
  "description": "Generates React component files automatically",
  "version": "0.0.3",
  "publisher": "abdullahceylan",
  "author": {
    "name": "Abdullah Ceylan",
    "email": "ben@abdullahceylan.com"
  },
  "homepage": "https://github.com/abdullahceylan/vscode-react-component-generator",
  "galleryBanner": {
    "color": "#0273D4",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdullahceylan/vscode-react-component-generator.git"
  },
  "bugs": {
    "url": "https://github.com/abdullahceylan/vscode-react-component-generator/issues",
    "email": "ben@abdullahceylan.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "vscode-react",
    "react",
    "react component",
    "react generate component",
    "redux",
    "stateless",
    "typescript"
  ],
  "activationEvents": [
    "onCommand:extension.genReactContainerComponentFiles",
    "onCommand:extension.genReactStatelessComponentFiles",
    "onCommand:extension.genReactReduxContainerComponentFiles",
    "onCommand:extension.genReactReduxStatelessComponentFiles"
  ],
  "main": "./out/src/extension",
  "icon": "assets/images/icon.png",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "AC React Component Generator",
      "properties": {
        "ACReactComponentGenerator.mainFile.create": {
          "type": "boolean",
          "default": true,
          "description": "Whether to generate component's main file or not. e.g.: ComponentName.(extension)"
        },
        "ACReactComponentGenerator.mainFile.extension": {
          "type": "string",
          "default": "jsx",
          "description": "The extension of generated component file"
        }
      }
    },
    "commands": [
      {
        "command": "extension.genReactContainerComponentFiles",
        "title": "New React Class Component"
      },
      {
        "command": "extension.genReactStatelessComponentFiles",
        "title": "New React Functional Component"
      },
      {
        "command": "extension.genReactReduxContainerComponentFiles",
        "title": "New React Class Component (Redux)"
      },
      {
        "command": "extension.genReactReduxStatelessComponentFiles",
        "title": "New React Functional Component (Redux)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.genReactContainerComponentFiles",
          "group": "navigation"
        },
        {
          "command": "extension.genReactStatelessComponentFiles",
          "group": "navigation"
        },
        {
          "command": "extension.genReactReduxContainerComponentFiles",
          "group": "navigation"
        },
        {
          "command": "extension.genReactReduxStatelessComponentFiles",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "mocha": "^2.3.3",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "change-case": "^3.0.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^0.30.0",
    "rxjs": "^5.0.0-rc.1",
    "lodash": "^4.17.11"
  }
}
